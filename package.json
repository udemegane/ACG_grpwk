{
  "name": "babylonjs-editor-generated-template",
  "version": "4.0.5-beta.4",
  "description": "Generated Template by BabylonJS Editor v4.0",
  "scripts": {
    "clean": "rimraf build && rimraf declaration",
    "compile": "tsc -p .",
    "build": "webpack --mode production",
    "build-noeditor": "webpack --config webpack.config.noeditor.js --mode production",
    "pack": "mkdir tmp && mv dist/* tmp/ && mkdir dist/dist && mv tmp/* dist/dist/ && cp -r scenes/ dist/ && rm -rf tmp && mv index.html dist/index.html && cp ./node_modules/cannon/build/cannon.js ./dist/ && sed -i -e 's/.\\/node_modules\\/cannon\\/build\\/cannon.js/.\\/cannon.js/g' ./dist/index.html",
    "watch": "webpack --mode development --watch",
    "webserver": "http-server -p 1338 -c-1"
  },
  "license": "(Apache-2.0)",
  "devDependencies": {
    "@tweakpane/core": "^1.0.7",
